                "enabled": false,
                "path": ""
            },
            "capabilities": {
                "platformName": "Android",
                "browserName": "Chrome",
                "appium:automationName": "uiautomator2",
                "bitbar:options": {
                    "apiKey": "<insert_your_apiKey_to_credentials_file>",
                    "device": "Google Pixel 3a Android 12",
